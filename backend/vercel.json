{
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "(.*)",
            "dest": "app.py"
        },
        {
            "src": "app/(.*)",
            "headers": {"Access-Control-Allow-Origin": "*"},
            "continue": true
        }
    ]
  }